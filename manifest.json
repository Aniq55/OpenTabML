{
	"manifest_version":2,
	"name": "OpenTabML",
	"version":"0.1",
	"permissions":[
		"history"
	],
	"content_scripts": [
	  {
	    "matches": [
	      "<all_urls>"
	    ],
	    "js": ["content.js"]
	  }
	],
	"background": {
	  "scripts": ["jquery-3.2.1.min.js","background.js"]
	}
